<externaltool id="Monitor_new">
    <description>Attach monitor to the device v4.0+</description>
    <displayname>Monitor_new</displayname>
    <category>ESP</category>
    <executable output="showinpane" error="showinpane" modifiesdocument="no">
        <path>gnome-terminal</path>
        <arguments>--working-directory %{CurrentProject:BuildPath} -- %{Env:IDF_PATH}/tools/idf.py monitor</arguments>
        <workingdirectory>%{CurrentProject:BuildPath}</workingdirectory>
        <baseEnvironmentId>CurrentBuild:Env</baseEnvironmentId>
    </executable>
</externaltool>
